{
  "hosting": {
    "public": "public",
    "rewrites": [ 
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/wall/*",
        "destination": "/index.html"
      }  
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
